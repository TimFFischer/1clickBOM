{
    "manifest_version": 2,
    
    "name": "1-click BOM",
    "description": "Automatically populate your electronic component shopping carts.",
    "version": "0.0.1",
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "browser_action": {
        "default_icon": "logo19.png"
    },
    "background": {
        "scripts": ["libs/domparser.js", "js/background.js"],
        "persistent": false
    },
    "web_accessible_resources": [ 
        "libs/qunit-1.11.0.css", 
        "libs/qunit-1.11.0.js"
    ],
    "icons": {"48":"logo48.png", "128":"logo128.png"}
    
}
